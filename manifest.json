{
  "manifest_version": 2,
  "name": "EMarts - Cookie Clicker chrome stoarge sync",
  "description": "Check your current sprint progress with one easy click",
  "version": "0.1",
  "icons": { "16": "img/icon16.png", "64": "img/icon64.png", "128": "img/icon128.png" },
  "browser_action": {
    "default_icon": "img/icon64.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "notifications",
    "activeTab",
    "storage",
    "tabs",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["app.js"],
    "persistent": false
  }
}
